{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "strict": true,
        "jsx": "react",
        "allowJs": true,
        "typeRoots": [ "./typings", "node_modules/@types" ],
    },
    "exclude": [
        "./node_modules/**/*", "typings"
    ]
}
